{
    "name": "@react-module-federation-webpack/source",
    "version": "0.0.0",
    "license": "MIT",
    "engines": {
        "node": "^18.19.1 || >=20.0.0",
        "npm": "Please install by running: npm i"
    },
    "scripts": {
        "start:host": "cross-env TARGET_ENV=local NODE_ENV=local GENERATE_SOURCEMAP=false npx nx run host:serve:local",
        "start:about": "cross-env TARGET_ENV=local NODE_ENV=local GENERATE_SOURCEMAP=false nx run about:serve:local",
        "start:shop": "cross-env TARGET_ENV=local NODE_ENV=local GENERATE_SOURCEMAP=false nx run shop:serve:local",
        "start:card": "cross-env TARGET_ENV=local NODE_ENV=local GENERATE_SOURCEMAP=false nx run cart:serve:local",
        "// - BUILD DEV": "Build Dev",
        "build:host:dev": "cross-env TARGET_ENV=dev nx run host:build:development",
        "build:about:dev": "cross-env TARGET_ENV=dev nx run about:build:development",
        "build:shop:dev": "cross-env TARGET_ENV=dev nx run shop:build:development",
        "build:card:dev": "cross-env TARGET_ENV=dev nx run cart:build:development",
        "// - BUILD STAGING": "Build STAGING",
        "build:host:staging": "cross-env TARGET_ENV=staging nx run host:build:staging",
        "build:about:staging": "cross-env TARGET_ENV=staging nx run about:build:staging",
        "build:shop:staging": "cross-env TARGET_ENV=staging nx run shop:build:staging",
        "build:card:staging": "cross-env TARGET_ENV=staging nx run cart:build:staging",
        "// - BUILD PRODUCTION": "Build Production",
        "build:all:prod": "npm run build:host:prod && npm run build:about:prod && npm run build:shop:prod && npm run build:card:prod",
        "build:host:prod": "npx nx run host:build:production --skip-nx-cache -- --base-href /react-module-federation-webpack/",
        "build:about:prod": "npx nx run about:build:production --skip-nx-cache -- --base-href /react-module-federation-webpack/",
        "build:shop:prod": "npx nx run shop:build:production --skip-nx-cache -- --base-href /react-module-federation-webpack/",
        "build:card:prod": "npx nx run cart:build:production --skip-nx-cache -- --base-href /react-module-federation-webpack/",
        "// - NX WORKSPACE": "nx command",
        "nx:graph": "npx nx graph --watch",
        "nx:reset": "npx nx reset",
        "nx:sync": "npx nx sync",
        "// - Mock API": "Mock API",
        "json-server": "json-server --routes routes.json --watch db.json --port 8081"
    },
    "private": true,
    "devDependencies": {
        "@babel/core": "^7.14.5",
        "@babel/preset-react": "^7.14.5",
        "@eslint/js": "^9.8.0",
        "@module-federation/enhanced": "^0.14.3",
        "@nx/cypress": "21.0.3",
        "@nx/eslint": "21.0.3",
        "@nx/eslint-plugin": "21.0.3",
        "@nx/jest": "21.0.3",
        "@nx/js": "21.0.3",
        "@nx/module-federation": "21.0.3",
        "@nx/react": "^21.0.3",
        "@nx/web": "21.0.3",
        "@nx/webpack": "21.0.3",
        "@nx/workspace": "21.0.3",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
        "@svgr/webpack": "^8.0.1",
        "@swc-node/register": "~1.9.1",
        "@swc/cli": "~0.6.0",
        "@swc/core": "~1.5.7",
        "@swc/helpers": "~0.5.11",
        "@testing-library/dom": "10.4.0",
        "@testing-library/react": "16.1.0",
        "@types/jest": "^29.5.12",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^20.0.0",
        "@types/react": "19.0.0",
        "@types/react-dom": "19.0.0",
        "autoprefixer": "10.4.13",
        "babel-jest": "^29.7.0",
        "cross-env": "^7.0.3",
        "cypress": "^14.2.1",
        "dotenv": "^16.5.0",
        "eslint": "^9.8.0",
        "eslint-config-prettier": "^10.0.0",
        "eslint-plugin-cypress": "^3.5.0",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-jsx-a11y": "6.10.1",
        "eslint-plugin-react": "7.35.0",
        "eslint-plugin-react-hooks": "5.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "json-server": "^0.17.4",
        "nx": "21.0.3",
        "postcss": "8.4.38",
        "prettier": "^2.6.2",
        "react-refresh": "^0.10.0",
        "tailwindcss": "3.4.3",
        "ts-jest": "^29.1.0",
        "ts-node": "10.9.1",
        "tslib": "^2.3.0",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.19.0"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.6.1",
        "axios": "^1.6.0",
        "classnames": "^2.5.1",
        "lodash-es": "^4.17.21",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-error-boundary": "^4.0.11",
        "react-redux": "^9.2.0",
        "react-router-dom": "6.29.0",
        "rxjs": "^7.8.1"
    }
}
